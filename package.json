{
  "name": "angular-cli-app-with-rollup",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngc-app-watch": "ngc -w -p src/tsconfig.app.json",
    "ngc-app": "ngc -p src/tsconfig.app.json",
    "rollup-clean-dev": "rimraf out-tsc && rimraf dist-rollup",
    "rollup-clean-prod": "rimraf out-tsc && rimraf dist-rollup-prod",
    "rollup-watch": "rollup -c -w --environment BUILD:dev",
    "rollup-dev": "npm run rollup-clean-dev && npm run ngc-app && npm run rollup-dev-app",
    "rollup-dev-app": "npm-run-all --parallel ngc-app-watch rollup-watch",
    "rollup-prod": "npm run rollup-clean-prod && npm run ngc-app && rollup -c --environment BUILD:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0-rc.3",
    "@angular/common": "9.0.0-rc.3",
    "@angular/compiler": "9.0.0-rc.3",
    "@angular/core": "9.0.0-rc.3",
    "@angular/forms": "9.0.0-rc.3",
    "@angular/platform-browser": "9.0.0-rc.3",
    "@angular/router": "9.0.0-rc.3",
    "core-js": "3.3.3",
    "rxjs": "6.5.3",
    "systemjs": "6.1.4",
    "tslib": "1.10.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.0-rc.3",
    "@angular/cli": "9.0.0-rc.3",
    "@angular/compiler-cli": "9.0.0-rc.3",
    "@angular/language-service": "9.0.0-rc.3",
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@types/jasmine": "3.4.4",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.11.5",
    "browser-sync": "2.26.7",
    "codelyzer": "5.2.0",
    "compression": "1.7.4",
    "fs-extra": "8.1.0",
    "http2": "3.3.7",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "npm-run-all": "4.1.5",
    "protractor": "5.4.2",
    "rollup": "1.25.2",
    "rollup-plugin-alias": "2.2.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-entrypoint-hashmanifest": "0.1.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-static-site": "0.1.0",
    "rollup-plugin-terser": "5.1.2",
    "strip-json-comments": "3.0.1",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "typescript": "3.6.4"
  }
}
